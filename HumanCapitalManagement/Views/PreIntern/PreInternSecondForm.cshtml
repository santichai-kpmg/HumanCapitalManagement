        function CalResult() {
            debugger;
            if (qCore1 != null && qCore2 != null && qCore3 != null && qCore4 != null && qCore5 != null && qCore6 != null && qCore7 != null && qCore8 != null && qCore9 != null) {
                calResult1 = qCore1 + qCore2 + qCore3;
                calResult2 = qCore4 + qCore5 + qCore6;
                calResult3 = qCore7 + qCore8 + qCore9;
                if (calResult1 != null) {
                    //Pass 1-1-1 / 1-1-4
                    if (calResult1 == 3 || calResult1 == 2) {
                        calResult1 = 1;
                    }
                    //Pending
                    else if (calResult1 == 1 || calResult1 == 0) {
                        calResult1 = 0;
                    }
                    else if (calResult1 >= 20) {
                        calResult1 = 20;
                    }
                }
                if (calResult2 != null) {
                    //Pass 1-1-1 / 1-1-4
                    if (calResult2 == 3 || calResult2 == 2) {
                        calResult2 = 1;
                    }
                    //Pending
                    else if (calResult2 == 1 || calResult2 == 0) {
                        calResult2 = 0;
                    }
                    //Notpass
                    else if (calResult2 >= 20) {
                        calResult2 = 20;
                    }
                }
                if (calResult3 != null) {
                    //Pass 1-1-1 / 1-1-4
                    if (calResult3 == 3 || calResult3 == 2) {
                        calResult3 = 1;
                    }
                    //Pending
                    else if (calResult3 == 1 || calResult3 == 0) {
                        calResult3 = 0;
                    }
                    //Notpass
                    else if (calResult3 >= 20) {
                        calResult3 = 20;
                    }
                }
                //Result Evaluation
                resultR = calResult1 + calResult2 + calResult3;
                //Pass
                if ( resultR == 3) {
                    $("#ddlPIAStatus option[value = 0]").attr('selected', 'selected');
                    document.getElementById("ddlPIAStatus").disabled = true;
                    $("#ddlPIAStatus").find('option[value = 1]').show();
                    $("#ddlPIAStatus").find('option[value = 3]').show();
                    GetSelectByID("ddlPIAStatus").val("1");

                }
                //Not pass or 2group is pending
                else if (resultR >= 20 || resultR == 1 || resultR==0) {
                    $("#ddlPIAStatus option[value = 0]").attr('selected', 'selected');
                    $("#ddlPIAStatus").find('option[value = 1]').hide();
                    $("#ddlPIAStatus").find('option[value = 3]').hide();
                    document.getElementById("ddlPIAStatus").disabled = true;
                    GetSelectByID("ddlPIAStatus").val("2");
                }
                    //pending 1 Grouphead
                else  {
                    $("#ddlPIAStatus option[value = 0]").attr('selected', 'selected');
                    $("#ddlPIAStatus").find('option[value = 1]').hide();
                    $("#ddlPIAStatus").find('option[value = 3]').show();
                    document.getElementById("ddlPIAStatus").disabled = false;
                    GetSelectByID("ddlPIAStatus").val("3");


                }
            }
        }

