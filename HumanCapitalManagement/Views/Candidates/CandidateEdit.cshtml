                };

                $.ajax({
                    dataType: AjaxCall.dataType,
                    type: AjaxCall.type,
                    contentType: AjaxCall.contentType,
                    url:'@Url.Action("DeleteCandidateTechnicalTest", "Candidates")',
                    data: JSON.stringify({ ItemData : Param }),
                    success: function (respone) {
                        if (respone.result.Status == SysProcess.SessionExpired) {
                            PopupLogin('@Url.Action("Index", "Home")');
                        } else if (respone.result.Status == SysProcess.Success) {
                            DialogSuccess(DialogHeader.Info, DialogMsg.SaveComplete);
                            if (respone.result.lstTechnicalTestTransaction != undefined && respone.result.lstTechnicalTestTransaction != null && respone.result.lstTechnicalTestTransaction.length > 0) {
                                $('#gvwDataTechnicalTest').DataTable().clear().draw();
                                $('#gvwDataTechnicalTest').dataTable().fnAddData(respone.result.lstTechnicalTestTransaction);
                                $('#gvwDataTechnicalTest').dataTable().fnDraw();
                            }
                            else {
                                $('#gvwDataTechnicalTest').DataTable().clear().draw();
                                $('#gvwDataTechnicalTest').dataTable().fnDraw();
                            }

                        }
                        else if (respone.result.Status == SysProcess.Duplicate){
                            DialogWarning(DialogHeader.Warning, respone.result.Msg);
                        }
                        else {
                            DialogWarning(DialogHeader.Warning, respone.result.Msg);
                        }


                    },
                    error: AjaxCall.error,
                    complete: function (jqXHR, status) {//finaly
                        UnblockUI();
                        $('#gvwDataTechnicalTest').DataTable().columns.adjust().draw();
                        $('#gvwDataTechnicalTest').DataTable().columns.adjust().responsive.recalc();
                        $("#PopupTechnicalTest").modal('hide');
                    }
                });
            }
        }


