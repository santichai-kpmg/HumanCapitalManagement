    <![endif]-->
    <link href="~/Content/Site.css" rel="stylesheet" />
    <link rel="icon" type="image/png"
          href="~/favicon.ico">
    <style>
        .form-group [class*="col-"] {
            margin-bottom: -5px;
            padding-bottom: 1px;
        }

        /*div > select .selectContainer {
            .form-control-feedback;

        {
            right: 25px !important;
        }

        }*/
        hr {
            border-top-color: #c5c2c2 !important;
            margin-bottom: 5px !important;
            margin-top: 10px !important;
            width: 98%;
        }

        .ui-autocomplete {
            max-height: 300px;
            overflow-y: auto;
            /* prevent horizontal scrollbar */
            overflow-x: hidden;
        }

        .row-search {
            padding-left: 10px;
            padding-right: 10px;
        }

        .oper-label {
            text-align: left !important;
        }

        .img-circle {
            image-rendering: -moz-crisp-edges; /* Firefox */
            image-rendering: -o-crisp-edges; /* Opera */
            image-rendering: -webkit-optimize-contrast; /* Webkit (non-standard naming) */
            -ms-interpolation-mode: nearest-neighbor; /* IE (non-standard property) */
        }

        /*.user-image img {
            position: absolute;
            left: 50%;
            top: 50%;
            height: 100%;
            width: auto;
            -webkit-transform: translate(-50%,-50%);
            -ms-transform: translate(-50%,-50%);
            transform: translate(-50%,-50%);
        }

            .user-image img.portrait {
                width: 100%;
                height: auto;
            }
        .img-circle img {
            position: absolute;
            left: 50%;
            top: 50%;
            height: 100%;
            width: auto;
            -webkit-transform: translate(-50%,-50%);
            -ms-transform: translate(-50%,-50%);
            transform: translate(-50%,-50%);
        }

            .img-circle img.portrait {
                width: 100%;
                height: auto;
            }*/
        figure {
            height: 90px;
            overflow: hidden;
            margin: 0;
        }

            figure img {
                display: block;
                height: 100%;
            }

        .change-ratio img {
        }
    </style>
</head>

<body class="hold-transition skin-green sidebar-mini @ViewBag.Menu">
    <div class="scrollToTop circle"><i class="fa fa-arrow-up"></i></div>
    <div class="wrapper">
        <header class="main-header">
            <!-- Logo -->
            <a href="@Url.Action("Index", "Home", null)" class="logo">
                <span class="logo-mini"><img src="~/Image/KPlogo_50x50.png" /></span>
                <span class="logo-lg"><img src="~/Image/KPlogo_200x50new.png" /></span>
            </a>
            <nav class="navbar navbar-static-top">
                <a href="#" class="sidebar-toggle" data-toggle="push-menu" role="button">
                    <span class="sr-only">Toggle navigation</span>
                </a>
                <div class="navbar-custom-menu">
                    <ul class="nav navbar-nav">
                        <li class="dropdown user user-menu">
                            <a href="#" class="dropdown-toggle" data-toggle="dropdown">
                                <img src="@ViewBag.UserPic" class="user-image">

                                <span class="hidden-xs">@ViewBag.UserName</span>
                            </a>
                            <ul class="dropdown-menu">
                                <!-- User image -->
                                <li class="user-header">
                                    <img src="@ViewBag.UserPic" class="img-circle">
                                    <p>
                                        @ViewBag.UserName - @ViewBag.Group
                                        <small>@ViewBag.Rank </small>
                                    </p>
                                </li>
                                <li class="user-footer">
                                    <div class="pull-right">
                                        <a href="@Url.Action("Login", "Login", null)" class="btn btn-default btn-flat">Change Role</a>
                                    </div>
                                </li>
                            </ul>
                        </li>
                    </ul>
                </div>
            </nav>
        </header>
        <aside class="main-sidebar">
            <section class="sidebar">
                @*@{ Html.RenderAction("CreMenu", "MasterPage");}*@
                <ul class="sidebar-menu" data-widget="tree">
                    <li>
                        <a href="@Url.Action("Index", "Home", null)">
                            <i class="fa fa-home"></i> <span>Home</span>
                        </a>
                    </li>
                </ul>
            </section>
        </aside>
        <div class="content-wrapper">
            @RenderBody()

        </div>
        <footer class="main-footer">
        
        </footer>
  
        <div class="control-sidebar-bg"></div>
    </div>
    <!-- ./wrapper -->

    <!-- jQuery UI 1.11.4 -->

    <!-- Resolve conflict in jQuery UI tooltip with Bootstrap tooltip -->
    <script>
        $.widget.bridge('uibutton', $.ui.button);
    </script>
    <!-- Bootstrap 3.3.6 -->
    @*<script src="bootstrap/js/bootstrap.min.js"></script>*@

    <!-- Morris.js charts -->

    <script src="~/Scripts/plugins/morris/morris.min.js"></script>
    <!-- Sparkline -->
    <script src="~/Scripts/plugins/sparkline/jquery.sparkline.min.js"></script>
    <!-- jvectormap -->
    <script src="~/Scripts/plugins/jvectormap/jquery-jvectormap-1.2.2.min.js"></script>
    <script src="~/Scripts/plugins/jvectormap/jquery-jvectormap-world-mill-en.js"></script>
    <!-- jQuery Knob Chart -->
    <script src="~/Scripts/plugins/knob/jquery.knob.js"></script>
    <!-- daterangepicker -->
    @*<script src="https://cdnjs.cloudflare.com/ajax/libs/moment.js/2.11.2/moment.min.js"></script>
        <script src="~/Scripts/plugins/daterangepicker/daterangepicker.js"></script>*@
    <!-- datepicker -->
    <script src="~/Scripts/plugins/datepicker/bootstrap-datepicker.js"></script>
    @*<script src="~/Scripts/plugins/bootstrap-datepicker-thai/js/bootstrap-datepicker.js"></script>*@
    <!-- Bootstrap WYSIHTML5 -->
    <script src="~/Scripts/plugins/bootstrap-wysihtml5/bootstrap3-wysihtml5.all.min.js"></script>
    <!-- Slimscroll -->
    <script src="~/Scripts/plugins/slimScroll/jquery.slimscroll.min.js"></script>
    <!-- FastClick -->
    <script src="~/Scripts/plugins/fastclick/fastclick.js"></script>
    <!-- AdminLTE App -->

    <link href="~/Scripts/plugins/iCheck/all.css" rel="stylesheet" />
    <script src="~/Scripts/plugins/iCheck/icheck.min.js"></script>

    <script src="~/Scripts/plugins/bootstrap3-dialog-master/js/bootstrap-dialog.js"></script>
    <script src="~/Scripts/SystemFunc/jquery.blockUI.js"></script>
    @*datatable net*@
    @*<script src="~/Scripts/app.js"></script>*@
    <script src="~/Scripts/SystemFunc/adminlte.js"></script>
    <script src="~/Scripts/SystemFunc/sysFunction.js"></script>
    <script src="~/Scripts/SystemFunc/CommonScript.js"></script>
    <!-- AdminLTE dashboard demo (This is only for demo purposes) -->
    @*<script src="dist/js/pages/dashboard.js"></script>*@
    <!-- AdminLTE for demo purposes -->
    @*<script src="dist/js/demo.js"></script>*@
    @RenderSection("style", required: false)
    @RenderSection("scripts", required: false)
    @RenderSection("scripts2", required: false)
    <script type="text/javascript">
        $(function () {
            $('select.selectContainer').parent().find('i.form-control-feedback').css("right", "25px");
        });
        $(document).ajaxStart(function () {
            BlockUI();
        });
        $(document).ajaxStop(function () {
            UnblockUI();
        });
    </script>
</body>
</html>


