                        //initialize icheck to the element
                        $(element).iCheck({
                            radioClass: 'iradio_flat-green'
                        });
                        $(element).on('ifChecked', function (event) {
                            var observable = valueAccessor();
                            observable.checked(event.target.defaultValue); //assigning selected value
