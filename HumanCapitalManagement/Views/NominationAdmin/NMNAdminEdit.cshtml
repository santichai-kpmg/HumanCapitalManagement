                        $('#gvwDataOld').DataTable().columns.adjust().draw();
                        $('#gvwDataOld').DataTable().columns.adjust().responsive.recalc();
                    }, 500);
                }
                else {
                    $('#gvwDataOld').DataTable().clear().draw();
                    $('#gvwDataOld').dataTable().fnDraw();
                }
                if (objModel.lstNewData != undefined && objModel.lstNewData != null && objModel.lstNewData.length > 0) {
