    <script type="text/javascript">
        $(function () {
            $('select.selectContainer').parent().find('i.form-control-feedback').css("right", "25px");
            $('section[class=content-header]').on('keydown', function (e) {
                if (e.which == 13) { //e.which = keyCode (ENTER : 13)
                    if (typeof LoadData == 'function') {

                        try {
                            LoadData();
                        }
                        catch (err) {
                            // Handle error(s) here

                        }
                    }
                    //e.preventDefault();
                }

            });
        });
        $(document).ajaxStart(function () {
            BlockUI();
        });
        $(document).ajaxStop(function () {
            UnblockUI();
        });
        function SetOpenMenu(sVal) {

            $.ajax({
                url: '@Url.Action("SetOpenMenu", "Base")',
                type: "POST",
                data: '{sVal: ' + sVal + ' }',
                contentType: "application/json; charset=utf-8",
                dataType: "json",
                success: function (result) {
                    BlockUI();
                    setTimeout(function () {
                        $('table').each(function () {
                            var ID = $(this)[0].id;
                            if (ID != undefined && ID + "" != "") {
                                if ($.fn.DataTable.isDataTable('#' + ID + '')) {
                                    $('#' + ID + '').DataTable().columns.adjust().draw();
                                    $('#' + ID + '').DataTable().columns.adjust().responsive.recalc();
                                }
                            }
                        });
                    }, 500);
                    //$('table').each(function () {
                    //    var ID = $(this)[0].id;
                    //    if (ID != undefined && ID + "" != "") {
                    //        if ($.fn.DataTable.isDataTable('#' + ID + '')) {
                    //            $('#' + ID + '').DataTable().columns.adjust().draw()
                    //        }
                    //    }
                    //});
                },
                error: function (err) {
                    alert(err.statusText);
                }, complete: function (jqXHR, status) {//finaly

                    setTimeout(function () {
                        $('table').each(function () {
                            var ID = $(this)[0].id;
                            if (ID != undefined && ID + "" != "") {
                                if ($.fn.DataTable.isDataTable('#' + ID + '')) {
                                    $('#' + ID + '').DataTable().columns.adjust().draw();
                                    $('#' + ID + '').DataTable().columns.adjust().responsive.recalc();
                                }
                            }
                        });
                    }, 500);
                    UnblockUI();
                }
            });
        }
        function RefreshTable(ctl) {
            var sBox = $(ctl).closest(".box");
            sBox.append("<div class='overlay'> <i class='fa fa-refresh fa-spin' ></i></div >");
            $('table').each(function () {
                var ID = $(this)[0].id;
                if (ID != undefined && ID + "" != "") {
                    if ($.fn.DataTable.isDataTable('#' + ID + '')) {
                        $('#' + ID + '').DataTable().columns.adjust().draw();
                        $('#' + ID + '').DataTable().columns.adjust().responsive.recalc();
                    }
                }
            });
            setTimeout(function () {
                $(".overlay").remove();
            }, 250);
            //$(".container").append($("h2"));
        }
    </script>
