
@model HumanCapitalManagement.ViewModel.MainVM.vPRCan_file

<div class="modal-body">
    <form class="form-horizontal" autocomplete="off">
        <div id="divUploadDoc">
            <div class="row">
                <div class="col-sm-12 " >
                    <div class="form-group form-group-sm">
                        <label class="col-sm-2 control-label text-right require">Import xlsx file</label>
                        <div class="col-sm-8  form-inline">
                            <div class="input-group input-group-sm">
                                <label class="input-group-btn">
                                    <span class="btn btn-primary">
                                        Browse&hellip;

                                        <input type="file" id="btnUpload3" name="files[]" style="display: none;" accept="application/vnd.openxmlformats-officedocument.spreadsheetml.sheet" />
                                    </span>
                                </label>
                                <input type="text" class="form-control" readonly />
                            </div>
                            <span class="help-block"><a href="~/Manual/CandidateUploadExp.xlsx" target="_blank" tooltip="File Example" flow="right"><img src="~/Image/filetype_xls.png" height="20"></a> Support .xlsx,  Only</span>
                        </div>
                    </div>
                </div>
            </div>
            <div class="row">
                <div class="col-md-12">
                    <div class="form-group form-group-sm">
                        <label class="col-md-4 control-label text-right ">Recruitment Team<font color=red>*</font> :</label>
                        <div class="col-md-6">
                            @{ Html.RenderAction("CreateddlRecruitment_Team", "HCMMasterData", new
                                {
                                    id = "ddlRecruitmentExl",

                                    value = Model.empNo + "",
                                });}
                        </div>
                    </div>
                </div>
            </div>
            <div class="row">
                <div class="col-md-12">
                    <div class="table-responsive">
                        <table class="table table-striped table-bordered table-hover display" id="gvwDataExcel" width="100%">
                            <thead>
                            </thead>
                            <tbody></tbody>
                        </table>
                    </div>
                </div>
            </div>
        </div>
    </form>
</div>
<div class="modal-footer">
    <button id="btnUpdate" type="button" onclick="AddExcelCandidate()" class="btn btn-primary">Add</button>
    <button type="button" class="btn btn-default" data-dismiss="modal">Close</button>
</div>
<script type="text/javascript">



</script>
